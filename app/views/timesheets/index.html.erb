<!--CONTENT STARTS-->
    <div class="content">
      <!--BREADDRUMBS STARTS-->
      <div id="breadcrumbs"> <a href="index.html">Home </a> / <a href="timesheet_timesheets.html">Timesheets</a> / Search</div>
      <!--BREADDRUMBS ENDS-->
      <!--FORM CONTAINER STARTS-->
      <div class="formContainer">
        <form action="#" method="get" enctype="multipart/form-data">
          <div class="pagedivider1">
          <fieldset>
          <div class="headingBg">
            <div class="fl w30">
              <h2>Edit Timesheets</h2>
            </div>
            <div class="clear"></div>
          </div>
          <table width="137%" border="1" cellpadding="0" cellspacing="0">
            <tr class="head">
              <td  align="left"><strong>Begin Date</strong></td>
              <td align="center"><strong>End Date</strong></td>
              <td align="center"><strong>Description</strong></td>
              <td align="center"><strong>Date Entered</strong></td>
              <td align="center"><strong>Status</strong></td>
            </tr>
       
            <% @timesheets.each do |timesheet| %>
            <tr>
              
              <td  align="left" width="24%"><%= link_to timesheet.start_date, edit_timesheet_url(timesheet) %></td>
              <td align="center" width="25%"><%= h timesheet.end_date %></td>
              <td align="center" width="25%"><%= h timesheet.description %></td>
              <td align="center" width="25%"><%= h timesheet.created_at %></td>
              <td align="center" width="25%"><%= h timesheet.status %></td>
            </tr>
            <% end %>
          </table>
          <div class="clear"></div>
          </fieldset>
        </form>
      </div>
      <div class="buttonMainWrapper pt25 pr25 pl25 pb25">
        <div class="buttonCancel">
          <form id="form1" name="form1" method="post" action="timesheet_timesheets.html">
            <input type="submit" class="buttonSubmit" value="Cancel" tabindex="" />
          </form>
        </div>
      </div>
    </div>